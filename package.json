    "laravel-elixir-webpack-official": "^1.0.2",
    "laravel-mix": "^4.0.15",
    "lodash": "^4.16.2",
    "resolve-url-loader": "2.3.1",
    "vue": "^2.0.1",
    "vue-resource": "^1.0.3"
  },
